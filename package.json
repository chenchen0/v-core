{
    "name": "v-core",
    "description": "基于Vue,ElementUI开发的一组页面组件包，目前包含Table、Form（在vue-form-making-h5的基础上修改而来）",
    "version": "1.0.13",
    "author": "Abe",
    "license": "none",
    "keywords": [
        "component",
        "vue",
        "table",
        "form",
        "element-ui",
        "auto",
        "lss"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/chenchen0/v-core"
    },
    "main": "src/index.js",
    "scripts": {
        "serve": "vue-cli-service serve --open",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "build-bundle": "vue-cli-service build --target lib --name VCore ./src/indexBundle.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "clipboard": "^2.0.1",
        "core-js": "^2.6.12",
        "element-ui": "^2.15.5",
        "exceljs": "^4.2.1",
        "file-saver": "^2.0.5",
        "jquery": "^3.6.0",
        "jsoneditor": "^5.24.6",
        "lodash": "^4.17.21",
        "multiparty": "^4.2.1",
        "normalize.css": "^8.0.0",
        "qiniu": "^7.2.1",
        "qiniu-js": "^2.5.1",
        "regenerator-runtime": "^0.13.9",
        "three": "^0.131.3",
        "tinymce": "^4.8.4",
        "v-ace-editor": "^1.2.4",
        "vant": "^1.4.8",
        "viewerjs": "^1.10.1",
        "vue": "^2.5.16",
        "vue-awesome": "^3.0.0",
        "vue-router": "^3.0.1",
        "vuedraggable": "^2.16.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@vue/cli-plugin-babel": "^3.0.0",
        "@vue/cli-plugin-eslint": "^3.0.0",
        "@vue/cli-service": "^3.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "node-sass": "^4.9.0",
        "rollup": "^0.57.1",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-buble": "^0.19.2",
        "rollup-plugin-uglify-es": "0.0.1",
        "rollup-plugin-vue": "^4.3.2",
        "sass-loader": "^7.0.1",
        "vue-template-compiler": "^2.5.13"
    },
    "babel": {
        "presets": [
            "@vue/app"
        ]
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "plugin:vue/essential"
        ]
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
